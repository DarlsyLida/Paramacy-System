        value7= StringVar()
        value8= StringVar()
        value9= StringVar()
        value10= StringVar()
        value11= StringVar()
        value12= StringVar()


        def Mark_Register():
            if value0.get()=="/":
                value1.set("/")
                value2.set("/")
                value3.set("/")
                value4.set("/")
                value5.set("/")
                value6.set("/")
                value7.set("/")
                value8.set("/")
                value9.set("/")
                value10.set("/")
                value11.set("/")

            elif value0.get()=="0":
                value1.set("0")
                value2.set("0")
                value3.set("0")
                value4.set("0")
                value5.set("0")
                value6.set("0")
                value7.set("0")
                value8.set("0")
                value9.set("0")
                value10.set("0")
                value11.set("0")

            elif value0.get()=="L":
                value1.set("L")
                value2.set("L")
                value3.set("L")
                value4.set("L")
                value5.set("L")
                value6.set("L")
                value7.set("L")
                value8.set("L")
                value9.set("L")
                value10.set("L")
                value11.set("L")

            elif value0.get()=="A":
                value1.set("A")
                value2.set("A")
                value3.set("A")
                value4.set("A")
                value5.set("A")
                value6.set("A")
                value7.set("A")
                value8.set("A")
                value9.set("A")
                value10.set("A")
                value11.set("A")

            elif value0.get()=="S":
                value1.set("S")
                value2.set("S")
                value3.set("S")
                value4.set("S")
                value5.set("S")
                value6.set("S")
                value7.set("S")
                value8.set("S")
                value9.set("S")
                value10.set("S")
                value11.set("S")
                
            elif value0.get()==" ":
                value1.set(" ")
                value2.set(" ")
                value3.set(" ")
                value4.set(" ")
                value5.set(" ")
                value6.set(" ")
                value7.set(" ")
                value8.set(" ")
                value9.set(" ")
                value10.set(" ")
                value11.set("")


        def Reset():
            value0.set("")
            value1.set("")
            value2.set("")
            value3.set("")
            value4.set("")
            value5.set("")
            value6.set("")
            value7.set("")
            value8.set("")
            value9.set("")
            value10.set("")
            value11.set("")

        def iExit():
            iExit = messagebox.askyesno("Exit PatientRegistration System","Do you want?")
            if iExit>0:
                root.destroy()
                return
            
